    "styled-loader": "^0.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "uglify-js": "^3.18.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0"
  },
  "license": "MIT",
