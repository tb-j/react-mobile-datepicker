{
  "name": "react-mobile-datepicker-ts",
  "version": "1.0.6",
  "types": "dist",
  "description": "react based date picker like native",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "dist/**/*"
  ],
  "amdName": "reactMobileDatePickerTs",
  "repository": "jin60641/react-mobile-datepicker",
  "homepage": "https://github.com/jin60641/react-mobile-datepicker#readme",
  "keywords": [
    "react-component",
    "react-datepicker",
    "date",
    "datepicker",
    "react",
    "mobile",
    "calendar"
  ],
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run clean && npm run transpile && npm run size",
    "transpile": "webpack --config ./webpack.build.config.js",
    "size": "size=$(gzip-size $npm_package_main) && echo \"gzip size: $size\"",
    "lint": "eslint -c .eslintrc.js '{lib,examples}/**/*.{ts,tsx}'",
    "start": "webpack serve"
  },
  "author": "rainie",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.42.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.3",
    "gzip-size-cli": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.39",
    "postcss-custom-properties": "^13.3.11",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^6.0.1",
